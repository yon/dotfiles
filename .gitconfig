[alias]
	a = add -A
	cam = commit -a -m
	ci = commit -v
	cim = commit -m
	co = checkout
	cp = cherry-pick -x
	d = diff --full-index
	l = log -c --no-merges
	lp = log -c -p --full-index --ignore-all-space --no-merges
	ls = log -c --no-merges --stat
	me = merge
	pl = pull
	ps = push
        rtag = !git tag r`date \"+%Y-%m-%d-%H%M%S\"`
	s = status
        tree = log --all --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
	ui = auto
[core]
	autocrlf = input
